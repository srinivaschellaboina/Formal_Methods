example (P : α → Prop) (Q : Prop) : ((∃ x, P x) ∧ Q) ↔ ∃ x, (P x ∧ Q) := by
 constructor 
 . intro temp
   obtain ⟨temp1, 
   temp2⟩ := temp
   obtain ⟨a, 
   tempa⟩ := temp1
   use a
   constructor
   apply tempa
   apply temp2